.idea/**/tasks.xml
.idea/**/shelf/
.idea/**/dictionaries/
.idea/**/usage.statistics.xml

# Local history and caches
.idea/**/localHistory/

# Recommended: ignore build output directories
/out/

# macOS Finder file
.DS_Store

